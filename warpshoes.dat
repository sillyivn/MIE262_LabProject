# Read each relevant table into sets and parameters

# [] represents the column you treat as an index 
# _ ~ ___ represents the values at corresponding to index 

table Shoe_Table "CSV" "Product_Master.csv":
    SHOE_TYPE <- [Product_Num], P ~ Sales_Price;
read table Shoe_Table;

table Machine_Master "CSV" "Machine_Master.csv":
    MACHINE_TYPE <- [Machine_Num], C_m ~ OpCost_per_min;
read table Machine_Master;

table RM_Master "CSV" "RM_Master.csv":
    RAW_MATERIAL_TYPE <- [RM_Num], C_r ~ Cost;
read table RM_Master;

# Import overall forecasted demand for each store
table Product_Demand "CSV" "Lab - Clean Forecasted Demand":
    SHOE_TYPE <- [Product_Num], D ~ "Total shoe demand"; 
read table Product_Demand;
	
table Machine_Assign "CSV" "Machine_Assign.csv":
    MACHINE_TYPE, SHOE_TYPE <- [Machine_Num, Product_Num], t ~ Avg_Duration;
read table Machine_Assign;

table BOM "CSV" "BOM.csv":
    RAW_MATERIAL_TYPE, SHOE_TYPE <- [RM_Num, Product_Num], A ~ Quantity; 
read table BOM;

table Warehouse_Master "CSV" "Warehouse_Master.csv":
    WAREHOUSE_TYPE <- [Warehouse_Num], W ~ Capacity, C_w ~ Op_Cost;
read table Warehouse_Master;
	